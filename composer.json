{
    "name": "silvanite/novafieldcheckboxes",
    "description": "A Laravel Nova field to display a number of multi-select options using checkboxes.",
    "keywords": [
        "laravel",
        "nova",
        "input",
        "field",
        "checkbox",
        "checkboxes",
        "multiselect",
        "multiple choice"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Silvanite\\NovaFieldCheckboxes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Silvanite\\NovaFieldCheckboxes\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require-dev": {
        "laravel/nova": "^4.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
